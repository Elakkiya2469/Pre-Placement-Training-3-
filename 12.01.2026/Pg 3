class QueueArray {
    int front = 0, rear = -1;
    int size = 5;
    int[] queue = new int[size];

    void enqueue(int x) {
        if (rear == size - 1)
            System.out.println("Queue Overflow");
        else
            queue[++rear] = x;
    }

    void dequeue() {
        if (front > rear)
            System.out.println("Queue Underflow");
        else
            System.out.println(queue[front++]);
    }

    public static void main(String[] args) {
        QueueArray q = new QueueArray();
        q.enqueue(1);
        q.enqueue(2);
        q.dequeue();
    }
}
